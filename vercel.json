{
  "version": 2,
  "name": "solaris-backend",
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/seed/(.*)",
      "dest": "/index.js"
    },
    { 
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}